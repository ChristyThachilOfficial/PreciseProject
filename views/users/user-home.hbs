	<script>
		function addToWishlist(productId,prodPrice){
			let buttonId =productId+'12'
			console.log(buttonId)
			$.ajax({
				url:'/wishlist/'+productId+'/'+prodPrice,
				method:'get',
				success:(response)=>{
					if(response.added){
						document.getElementById(buttonId).style.color ='red'
						Swal.fire({
						  icon: 'success',
						  title: 'Item added to wishlist!!!!',
						  timer:1500
						 
						})
					}
					
				}
			})
		}
	</script>

